<?xml version="1.0"?>
<robot name="finger">
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://ohp_model_t_model/meshes/base_link.STL"/>
      </geometry>
      <material name="blue">
        <color rgba="0.7 0.6 0.4 0.8"/>
      </material>
    </visual>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
    <contact>
        <lateral_friction value="0."/>
        <inertia_scaling value="1.0"/>
        <spinning_friction value="0."/>
        <rolling_friction value="0."/>
        <stiffness value="100000"/>
        <damping value="0"/>
    </contact>
  </link>

  <joint name="base_to_finger1" type="revolute">
    <axis xyz="0 -1 0"/>
    <limit effort="1000.0" lower="-3.142" upper="3.142" velocity="0.5"/>
    <dynamics damping="0" friction="0"/>
    <origin rpy="0 0 3.14159" xyz="-0.01453 0.00902 0.05475"/>
    <parent link ="base_link"/>
    <child link="finger1_prox"/>
  </joint>

  <link name="finger1_prox">
    <visual>
      <geometry>
	    <mesh filename="package://ohp_model_t_model/meshes/finger_prox.STL"/>
      </geometry>
      <material name="blue">
        <color rgba="0.8 0.8 0.8 0.8"/>
      </material>
    </visual>
    <collision>
      <geometry>
	    <mesh filename="package://ohp_model_t_model/meshes/finger_prox.STL"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <contact>
        <lateral_friction value="0."/>
        <inertia_scaling value="1.0"/>
        <spinning_friction value="0."/>
        <rolling_friction value="0."/>
        <stiffness value="100000"/>
        <damping value="0"/>
    </contact>
  </link>

  <link name="finger1_dist">
    <visual>
      <geometry>
	    <mesh filename="package://ohp_model_t_model/meshes/finger_dist.STL"/>
      </geometry>
      <material name="blue">
        <color rgba="0.8 0.8 0.8 0.8"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<mesh filename="package://ohp_model_t_model/meshes/finger_dist.STL"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <contact>
        <lateral_friction value="0."/>
        <inertia_scaling value="1.0"/>
        <spinning_friction value="0."/>
        <rolling_friction value="0."/>
        <stiffness value="100000"/>
        <damping value="0"/>
    </contact>
  </link>

  <joint name="prox_to_dist1" type="revolute">
    <axis xyz="0 -1 0"/>
    <limit effort="1000.0" lower="-3.142" upper="3.142" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0.061 0 0.004"/>
    <parent link ="finger1_prox"/>
    <child link="finger1_dist"/>
    <dynamics damping="0" friction="0"/>
  </joint>

</robot>
